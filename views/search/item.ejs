<div class="card">
  <% var uid = item.uid %>
  <a href="/shops/<%= uid %>" class="text-body">
    <% if(typeof item.image == 'undefined'){ %>
    <img src="/public/image/default/286x180.png" class="card-img-top" alt="">
    <% } else {%>
    <img src="<%= item.image %>" class="card-img-top" alt="">
    <% } %>

    <div class="card-body">
      <h5 class="card-title over-text"><%= item.name %></h5>
      <% if(typeof item.genre[0].Name == 'undefined') {%>
      <p class="card-text">''</p>
      <% }else{ %>
      <p class="card-text"><%= item.genre[0].Name %></p>
      <% } %>
    </div>
  </a>
</div>